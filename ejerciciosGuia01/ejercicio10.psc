Proceso ejercicio10
	//declaracion vnles
	Definir alto Como Real;
	Definir ancho Como Real;
	Definir area Como Real;
	Definir perimetro Como Real;
	//entradas
	Escribir "ingrese el alto del rectangulo:";
	Leer alto;
	
	Escribir "ingrese el ancho del rectangulo";
	Leer ancho;
	// procesos-salidas
	area<- alto*ancho;
	Escribir "el area del rectangulo es : ", area;
	
	perimetro<- (2*ancho)+(2*alto);
	Escribir "el perimetro del rectangulo es:", perimetro;
	
	
FinProceso
